<ContentDialog x:Name="ReplaceOrKeepDialog1"
    x:Class="WeatherDataAnalysis.View.ReplaceOrKeepDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="Conflicting Data"
    PrimaryButtonText="Keep Original Data"
    SecondaryButtonText="Replace With New Data"
    PrimaryButtonClick="ContentDialog_PrimaryButtonClick"
    SecondaryButtonClick="ContentDialog_SecondaryButtonClick">

    <Grid>
        <TextBlock x:Name="DescriptionTextBlock" HorizontalAlignment="Center" Text="" TextWrapping="Wrap" VerticalAlignment="Center" Width="321"/>
        <CheckBox x:Name="DoForAllCheckBox" Content="Do for all" HorizontalAlignment="Center" Margin="0,70,0,0" VerticalAlignment="Top"/>
    </Grid>
</ContentDialog>
